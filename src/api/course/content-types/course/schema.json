{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "singularName": "course",
    "pluralName": "courses",
    "displayName": "Course",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "overview": {
      "type": "richtext",
      "required": true
    },
    "what_you_will_learn": {
      "type": "component",
      "repeatable": true,
      "component": "home-page.you-have-been-waiting-for",
      "required": true
    },
    "course_curriculum": {
      "type": "component",
      "repeatable": true,
      "component": "home-page.you-have-been-waiting-for"
    },
    "downloadable": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "faqs": {
      "type": "component",
      "repeatable": true,
      "component": "faq-questions.faq-questions",
      "required": true
    },
    "home": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "is_featured": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "required": true
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "videos"
      ]
    },
    "video_thumbnail": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "icon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "mini_name": {
      "type": "string",
      "required": false
    }
  }
}