{
  "kind": "singleType",
  "collectionName": "contact_pages",
  "info": {
    "singularName": "contact-page",
    "pluralName": "contact-pages",
    "displayName": "Contact Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "address": {
      "type": "component",
      "repeatable": true,
      "component": "contact-us.address"
    },
    "title": {
      "type": "string",
      "maxLength": 15,
      "required": true
    },
    "description": {
      "type": "text",
      "required": true,
      "maxLength": 100
    },
    "heading": {
      "type": "string",
      "maxLength": 15,
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "footer_description": {
      "type": "text",
      "required": true,
      "maxLength": 150
    },
    "SEO": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo",
      "required": true
    }
  }
}
